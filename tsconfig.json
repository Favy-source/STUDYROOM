{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(app)/layout.js", "app/components/SignUp.tsx", "app/components/Provider.tsx", "app/(root)/layout.js", "app/(auth)/layout.js", "app/(admin)/layout.js", "app/api/auth/login/route.js", "app/api/uploadthing/route.ts"],
  "exclude": ["node_modules"]
}
